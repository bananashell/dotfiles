{
  "workbench.colorTheme": "Community Material Theme High Contrast",
  "editor.fontFamily": "'Cartograph CF', monospace",
  "editor.fontLigatures": true,
  "git.autofetch": true,

  "editor.suggest.localityBonus": true,
  "editor.fontWeight": "400",
  // "terminal.integrated.shell.osx": "/opt/homebrew/bin/fish",
  "terminal.integrated.profiles.osx": {
    // "fish": {
    //   "path": "/opt/homebrew/bin/fish"
    // },
    // "bash": null,
    "pwsh": null
  },
  "terminal.integrated.fontFamily": "Hasklug Nerd Font",
  "remote.extensionKind": {
    "ms-azuretools.vscode-docker": "workspace"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.console.fontFamily": "Cartograph CF, monospace",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "workbench.list.smoothScrolling": true,
  "remote.containers.workspaceMountConsistency": "delegated",
  "scm.inputFontFamily": "editor",
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "editor.minimap.maxColumn": 30,
  "editor.minimap.renderCharacters": false,
  "editor.linkedEditing": true,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.enableBell": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.enableEditorConfigSupport": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "rest-client.followredirect": false,
  "editor.quickSuggestionsDelay": 0,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[csharp]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorSurroundingLines": 2,
  "editor.cursorWidth": 3,
  "terminal.integrated.cursorBlinking": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "terminal.integrated.defaultProfile.osx": "fish",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.color": "#90a4ae",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.saturation": null,
  "material-icon-theme.activeIconPack": "react",
  "workbench.colorCustomizations": {
    "[Material Theme Palenight High Contrast]": {
      "activityBarBadge.background": "#FFA000",
      "activityBar.activeBorder": "#FFA000",
      "list.activeSelectionForeground": "#FFA000",
      "list.inactiveSelectionForeground": "#FFA000",
      "list.highlightForeground": "#FFA000",
      "scrollbarSlider.activeBackground": "#FFA00050",
      "editorSuggestWidget.highlightForeground": "#FFA000",
      "textLink.foreground": "#FFA000",
      "progressBar.background": "#FFA000",
      "pickerGroup.foreground": "#FFA000",
      "tab.activeBorder": "#FFA000",
      "notificationLink.foreground": "#FFA000",
      "editorWidget.resizeBorder": "#FFA000",
      "editorWidget.border": "#FFA000",
      "settings.modifiedItemIndicator": "#FFA000",
      "settings.headerForeground": "#FFA000",
      "panelTitle.activeBorder": "#FFA000",
      "breadcrumb.activeSelectionForeground": "#FFA000",
      "menu.selectionForeground": "#FFA000",
      "menubar.selectionForeground": "#FFA000",
      "editor.findMatchBorder": "#FFA000",
      "selection.background": "#FFA00040",
      "statusBarItem.remoteBackground": "#FFA000"
    }
  },
  "materialTheme.accent": "Yellow",
  "files.exclude": {
    "**/.next": true,
    "**/.turbo": true,
    "**/bin": true,
    "**/node_modules": true,
    "**/obj": true
  },
  "redhat.telemetry.enabled": false,
  "powermode.enabled": true,
  "powermode.enableShake": false,
  "editor.inlineSuggest.enabled": true,
  "editor.quickSuggestions": {
    "other": "inline",
    "comments": "inline",
    "strings": "inline"
  },
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "comments.openView": "file",
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.automationProfile.osx": {},
  "window.confirmBeforeClose": "keyboardOnly",
  "handydandy-notebook.dispatch": {
    "python": ["python3", ["-c", "${code}"]],
    "typescript": ["ts-node", ["-T", "--skip-project", "-e", "${code}"]],
    "javascript": ["node", ["-e", "(async () => { ${code} } )()"]],
    "ruby": ["ruby", ["-e", "${code}"]],
    "shellscript": ["bash", ["-c", "${code}"]]
  },
  "files.associations": {
    "apple-app-site-association": "json"
  },
  "[arm-template]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false
  },
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.layoutControl.enabled": false,
  "editor.stickyScroll.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fill",
  "window.zoomLevel": 1
}
